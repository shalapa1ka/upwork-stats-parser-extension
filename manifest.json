{
  "name": "Upwork Request Forwarder",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["cookies", "tabs", "activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
